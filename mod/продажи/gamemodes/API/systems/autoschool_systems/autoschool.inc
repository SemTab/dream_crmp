stock get_lic_price(i, type)
{
	switch(type)
	{
		case 1:
		{
			if(pData[i][pLevel] < 3) 
				return 1000;

			else if(3 < pData[i][pLevel] < 7) 
				return 2500;

			else return 5000;
		}
		case 2:
		{
			if(pData[i][pLevel] < 3) 
				return 1500;
			else if(3 < pData[i][pLevel] < 7) 
				return 5000;

			else return 10000;
		}
		case 3:
		{
			if(pData[i][pLevel] < 3) 
				return 5000;

			else if(3 < pData[i][pLevel] < 7) 
				return 15000;

			else return 30000;
		}
	}
	return true;
}

