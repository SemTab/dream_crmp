stock autoschool_route(i, carid, route)
{
	if(vehicle[carid][cOwnTo] != VEHICLES_AUTOSCHOOL) return autoschool_end_lesson(i);
	
	new time = GetTickCount();


	new
		next_route = route + 1;


	switch(GetPVarIntNew(i, "autoschool_type"))
	{
		case 1:
		{

			if(route == sizeof autoschool_route_car - 1)
			{
				new Float:car_health;
				GetVehicleHealth(carid, car_health);

				if(car_health < 900)
				{
					SCM(i, -1, "");
					SendErr(i, "Вы повредили транспортное средство и провалили практический экзамен в автошколе!");

					DestroyVehicle(carid);

					DeletePVarNew(i, "autoschool_type");
					DeletePVarNew(i, "autoschool_route");
					DisablePlayerRaceCheckpoint(i);
				}
				else
				{
					SCM(i, -1, "");
					SendInf(i, "Поздравляем! Вы успешно сдали практический экзамен и получили водительские права!");
					SendInf(i, "Посмотреть свои лицензии можно через команду - /lic");

					pData[i][pLicenses][LIC_DRIVING] = 1;

					check_quest_prize_start(i, 10);
					check_quest_prize_start(i, 7);

					SetPlayerVirtualWorld(i, 0);
					
					UpdatePlayerData(i, "licDrive", 1);

					DestroyVehicle(carid);

					DeletePVarNew(i, "autoschool_type");
					DeletePVarNew(i, "autoschool_route");
					DisablePlayerRaceCheckpoint(i);
				}
			}
			else
			{
				random_phrase_exam_drive_v(i, route);
				SetPlayerRaceCheckpoint(i, 3, autoschool_route_car[route][0], autoschool_route_car[route][1], autoschool_route_car[route][2], autoschool_route_car[next_route][0], autoschool_route_car[next_route][1], autoschool_route_car[next_route][2] - 2.0, 4.0);
			}
		}
		case 2:
		{

			if(route == sizeof autoschool_route_boat - 1)
			{
				new Float:car_health;
				GetVehicleHealth(carid, car_health);
				if(car_health < 900)
				{
					SCM(i, -1, "");
					SendErr(i, "Вы повредили транспортное средство и провалили практический экзамен в автошколе!");

					DestroyVehicle(carid);

					DeletePVarNew(i, "autoschool_type");
					DeletePVarNew(i, "autoschool_route");
					DisablePlayerRaceCheckpoint(i);
				}
				else
				{
					SCM(i, -1, "");
					SendInf(i, "Поздравляем! Вы успешно сдали практический экзамен и получили права на управление водным транспортом!");
					SendInf(i, "Посмотреть свои лицензии можно через команду - /lic");

					pData[i][pLicenses][LIC_BOAT] = 1;

					SetPlayerVirtualWorld(i, 0);

					UpdatePlayerData(i, "licBoat", 1);

					check_quest_prize_start(i, 10);
					check_quest_prize_start(i, 9);

					DestroyVehicle(carid);
					SetPlayerPosEx(i, -451.5421,-48.9278,2.7685,0.8231);


					DeletePVarNew(i, "autoschool_type");
					DeletePVarNew(i, "autoschool_route");
					DisablePlayerRaceCheckpoint(i);
				}
			}
			else
			{
				random_phrase_exam_drive_b(i, route);
				SetPlayerRaceCheckpoint(i, 3, autoschool_route_boat[route][0], autoschool_route_boat[route][1], autoschool_route_boat[route][2], autoschool_route_boat[next_route][0], autoschool_route_boat[next_route][1], autoschool_route_boat[next_route][2], 4.0);
			}
		}
		case 3:
		{

			if(route == sizeof autoschool_route_fly - 1)
			{
				new Float:car_health;
				GetVehicleHealth(carid, car_health);
				if(car_health < 900)
				{
					SCM(i, -1, "");
					SendErr(i, "Вы повредили транспортное средство и провалили практический экзамен в автошколе!");

					RepairVehicle(carid);
					SetVehicleToRespawn(carid);

					DeletePVarNew(i, "autoschool_type");
					DeletePVarNew(i, "autoschool_route");
					DisablePlayerRaceCheckpoint(i);
				}
				else
				{
					SCM(i, -1, "");
					SendInf(i, "Поздравляем! Вы успешно сдали практический экзамен и получили права на управление воздушным транспортом!");
					SendInf(i, "Посмотреть свои лицензии можно через команду - /lic");

					pData[i][pLicenses][LIC_FLY] = 1;

					check_quest_prize_start(i, 10);
					check_quest_prize_start(i, 8);

					SetPlayerVirtualWorld(i, 0);

					UpdatePlayerData(i, "licFly", 1);

					DestroyVehicle(carid);

					DeletePVarNew(i, "autoschool_type");
					DeletePVarNew(i, "autoschool_route");
					DisablePlayerRaceCheckpoint(i);
				}
			}
			else
			{
				random_phrase_exam_drive_f(i, route);
				SetPlayerRaceCheckpoint(i, 3, autoschool_route_fly[route][0], autoschool_route_fly[route][1], autoschool_route_fly[route][2], autoschool_route_fly[next_route][0], autoschool_route_fly[next_route][1], autoschool_route_fly[next_route][2], 6.0);
			}
		}
	}
	route ++;

	if(debug_mode)
	{
		printf("[autoschool_route]: Времени затрачено: <%d мс>" , GetTickCount() - time);
	}
	
	return SetPVarIntNew(i, "autoschool_route", route);
}

stock autoschool_end_lesson(i)
{
	if(!GetPVarIntNew(i, "autoschool_type")) return true;
	new carid = GetPlayerVehicleID(i);
	DestroyVehicle(carid);

	DeletePVarNew(i, "autoschool_type");
	DeletePVarNew(i, "autoschool_route");
	DisablePlayerRaceCheckpoint(i);

	SendInf(i, "Практический экзамен завершен!");
	return true;
}




