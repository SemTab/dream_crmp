forward ContTimer(cont_id);
public ContTimer(cont_id)
{
    new string[216];
    if(g_cont[cont_id][C_TIME] > gettime())
    {
        format(string, 216, "{FFFFFF}Контейнер {66CC33}№%d\n{FFFFFF}Может содержать {66CC33}%s\n{FFFFFF}Текущая ставка {66CC33}%s от %s\n{66CC33} Поставить ставку /betcont\n(осталось %d секунд)",cont_id+1, g_cont[cont_id][C_NAME], formatInt(g_cont[cont_id][C_BET]), pData[g_cont[cont_id][C_LEADER_BET]][pNickname],g_cont[cont_id][C_TIME]-gettime());
    }
    else
    {
        new playerid = g_cont[cont_id][C_LEADER_BET];
        KillTimer(g_cont[cont_id][C_TIMER]);

        opencont[playerid]++;

        if(opencont[playerid] == 3)
        {
            //PI[playerid][pDonateRubNew] += 20;
            //PI[playerid][P_QUEST][3] = 1;
            //UpdatePlayerData(playerid, "pRub", PI[playerid][pDonateRubNew]);
            //UpdatePlayerData(playerid, "quest_3", PI[playerid][P_QUEST][3]);
        }

        g_cont[cont_id][C_BET] = g_cont[cont_id][C_PRICE];
        switch(g_cont[cont_id][C_TYPE_GIFT])
        {
            case 1:
            {
                new model = RandomEx(0, 20);
                SetPVarInt(g_cont[cont_id][C_LEADER_BET], "PriseCar", model);
                format(string, 64, "Модель: %s", transport_config[GiftCar[model]-400][transport_name]);
                ShowPlayerDialog
                (
                g_cont[cont_id][C_LEADER_BET],2325,DIALOG_STYLE_MSGBOX,
                string,
                "{ffffff}Вы действительно хотите забрать данный транспорт?",
                "Забрать","Деньги"
                );
            }
            case 2:
            {
                new model = RandomEx(0, 14);
                SetPVarInt(g_cont[cont_id][C_LEADER_BET], "PriseSkin", model);
                set_player_skin(playerid, GiftSkin[model]);
                ShowPlayerDialog
                (
                g_cont[cont_id][C_LEADER_BET],4007,DIALOG_STYLE_MSGBOX,
                string,
                "{ffffff}Вы действительно хотите забрать данный скин?",
                "Забрать","Деньги"
                );
            }
            case 3:
            {
                pData[playerid][pDonate] += RandomEx(1, 1500);
                UpdatePlayerData(playerid, "donate", pData[playerid][pDonate]);
                new stra[128];
                format(stra, sizeof stra, "Вы выиграли %d доната, они зачислены на ваш счёт", pData[playerid][pDonate]);
                SendSucc(playerid, stra);
            }

        }

        format(string, 186, "{FFFFFF}Контейнер {66CC33}№%d\n{FFFFFF}Может содержать {66CC33}%s\n{FFFFFF}Стартовая цена {66CC33}%s\n{66CC33} Поставить ставку /betcont",cont_id+1, g_cont[cont_id][C_NAME], formatInt(g_cont[cont_id][C_PRICE]));
        g_cont[cont_id][C_LEADER_BET] = -1;
	}
    UpdateDynamic3DTextLabelText(g_cont[cont_id][C_LABEL], 0xFFFF00FF, string);
    return 1;
}

