<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/sd_dialog_item"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/sd_dialog_item_row"
        android:paddingBottom="@dimen/_6sdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/_6sdp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <TextView
            android:textSize="@dimen/_12sdp"
            android:textColor="#FFFFFF"
            android:gravity="left"
            android:id="@+id/dialog_field1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Field1"
            android:maxLines="1"
            android:lines="1"
            android:singleLine="true"
            android:fontFamily="@font/akrobat_bold"
            android:paddingEnd="@dimen/_5sdp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <TextView
            android:textSize="@dimen/_12sdp"
            android:textColor="#FFFFFF"
            android:gravity="left"
            android:id="@+id/dialog_field2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:maxLines="1"
            android:lines="1"
            android:singleLine="true"
            android:fontFamily="@font/akrobat_bold"
            android:paddingEnd="@dimen/_5sdp"
            app:layout_constraintStart_toEndOf="@+id/dialog_field1"
            app:layout_constraintTop_toTopOf="parent" />
        <TextView
            android:textSize="@dimen/_12sdp"
            android:textColor="#FFFFFF"
            android:gravity="left"
            android:id="@+id/dialog_field3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:maxLines="1"
            android:lines="1"
            android:singleLine="true"
            android:fontFamily="@font/akrobat_bold"
            android:paddingEnd="@dimen/_5sdp"
            app:layout_constraintStart_toEndOf="@+id/dialog_field2"
            app:layout_constraintTop_toTopOf="parent" />
        <TextView
            android:textSize="@dimen/_12sdp"
            android:textColor="#FFFFFF"
            android:gravity="left"
            android:id="@+id/dialog_field4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:maxLines="1"
            android:lines="1"
            android:singleLine="true"
            android:fontFamily="@font/akrobat_bold"
            android:paddingEnd="@dimen/_5sdp"
            app:layout_constraintStart_toEndOf="@+id/dialog_field3"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>